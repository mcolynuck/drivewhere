<% @page_title = "Admin Login" %>

<div class="login">
	<%= form_tag(:action => 'attempt_login') do %>
	<ul>
		<li>
			<%= label_tag(:username) %>
			<%= text_field_tag(:username) %>
		</li>
		<li>
			<%= label_tag(:password) %>
			<%= password_field_tag(:password) %>
		</li>
	</ul>
	<%= submit_tag("Log In") %>
	<% end %>
</div>